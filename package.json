    "react-native-google-places-autocomplete": "1.3.9",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-maps": "^0.24.2",
    "react-native-web": "^0.11.4",
    "react-navigation": "3.0.7",
    "react-test-renderer": "^15.4.2"
