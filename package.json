    "babel-preset-react-native": "5.0.2",
    "dotenv": "^8.0.0",
    "enzyme": "^3.10.0",
    "eslint": "^6.0.1",
    "expo": "^33.0.0",
    "expo-location": "^5.0.1",
    "expo-permissions": "^5.0.1",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "prop-types": "^15.7.2",
    "react": "16.8.3",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^16.8.6",
